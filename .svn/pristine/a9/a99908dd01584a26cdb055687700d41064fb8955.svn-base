.main{
    padding: 0 20rpx;
}
.search-box {
    position: relative;
    display: flex;
    justify-content: space-between;
    padding-top: 26rpx;
}
.cancel{
  position: absolute;
  right: 63px;
  top: 25px;
  z-index: 99
}
.search-input {
    flex: 1;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx 0 54rpx;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 32rpx;
    border: 2rpx solid #ddd;
}

.search-icon {
    position: absolute;
    left: 15rpx;
    top: 44rpx;
    width: 35rpx;
    height: 35rpx;
}
.search-btn{
    font-size: 26rpx;
    padding: 0 20rpx;
    line-height: 64rpx;
    margin-left: 5px;
}
.search_list{
  display: flex;
  width: 100%;
  margin-top: 20px;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap
}
.search_zs{
  width: 325rpx;
  height: 370rpx;
  border-radius: 5px;
  background: #f5f6f5;
  position: relative;
  margin-bottom: 15rpx;
}
.search_zs .shopImg{
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx 65rpx 0 
}
.search_info{
  text-align: center;
  font-size: 32rpx;
}
.add{
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
  width: 45rpx;
  height: 45rpx;
}
.history_search{
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
}
.his_title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 34rpx;
}
.history{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.history_text{
  padding: 20rpx;
  height: 30rpx;
  font-size: 30rpx;
  background: #f3f3f3;
  line-height: 30rpx;
  margin: 16rpx;
}
.search-img image{
  width: 81rpx;
  height: 75rpx;
}
.txt{
  position: absolute;
  font-size: 14px;
  width: 18px;
  height: 18px;
  border-radius: 100%;
  background: red;
  color: white;
  top: 20px;
  right: 0;
  line-height: 16px;
  text-align: center;
}