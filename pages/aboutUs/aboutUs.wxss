/* pages/aboutUs/aboutUs.wxss */
page{
  height: 100%;
  background: #F2F2F2;
}
.item {
  border-bottom: 1px solid #d1d1d1;
  padding: 15px 10px;
  position: relative;
  background: white;
}

.arrow-img {
  position: absolute;
  width: 20px;
  height: 20px;
  top: 50%;
  right: 10px;
  margin-top: -10px;
}

.fixed{
  position: fixed;
  left: 50%;
  bottom: 40px;
  width: 120px;
  margin-left: -60px;
  font-size: 12px;
  color: #b3b3b3;
  text-align: center;
}

.logo-img{
  width: 60px;
  height: 60px;
}